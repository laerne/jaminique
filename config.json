{ "verbose" : false
, "number" : 1
, "infinity-threshold" : 10000
, "seed" : "auto"
, "gui" :
    { "enabled" : true
    }
, "lexicon" :
    { "uniform" : false
    , "ignore-case" : false
    , "files" :
      [ "namelists/viking_female.txt"
      , "namelists/viking_male.txt"
      , "namelists/viking_unknownGender.txt"
      ]
    }
, "generator" :
    { "default" : "markov"
    , "markov" :
        { "ngram-size" : 3
        }
    , "smooth-markov" :
        { "ngram-size" : 2
        }
    }
, "filters" :
    { "min-length" : 3
    , "max-length" : 24
    , "original" : true
    }
}

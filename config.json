{ "verbose" : false
, "number" : 1
, "infinity-threshold" : 10000
, "seed" : "auto"
, "gui" :
    { "enabled" : true
    }
, "lexicon" :
    { "files" :
      [ "lexicons" ]
    }
, "tokenizer" :
    { "default" : "unicode"
    , "presets" :
        { "unicode" :
            { "algorithm" : "unicode"
            }
        , "lower case" :
            { "algorithm" : "unicode"
            , "case" : "lower"
            }
        , "upper case" :
            { "algorithm" : "unicode"
            , "case" : "upper"
            }
        , "unidecode" :
            { "algorithm" : "unidecode"
            , "case" : "lower"
            }
        , "english phonemes" :
            { "algorithm" : "ll1"
            , "token-list" : "eau,eue,ewe,ieu,iew,oeu,aa,ah,ae,ai,ao,au,aw,ay,ee,ei,ey,eo,eu,ew,ie,oa,oe,oi,oo,ou,ow,oy,ue,ui,uu,uy,dge,dgi,dgy,pph,rrh,sce,sci,scy,sch,tch,xce,xci,bb,cc,ch,ck,dd,dh,ff,gg,gh,kk,kh,ll,mm,nn,ng,pp,ph,qu,rr,rh,sh,ss,sw,tt,th,vv,wh,wr,xh,zz,a,b,c,d,e,f,g,h,i,j,k,l,m,n,o,p,q,r,s,t,u,v,w,x,y,z"

            }
        }
    }
, "generator" :
    { "default" : "markov"
    , "presets" :
        { "markov" :
            { "algorithm" : "markov"
            , "ngram-size" : 3
            , "min-length" : 3
            , "truncation-length" : 48
            }
        , "smooth markov" :
            { "algorithm" : "smooth-markov"
            , "ngram-size" : 2
            , "min-length" : 3
            , "truncation-length" : 48
            }
        , "noise" :
            { "algorithm" : "markov"
            , "ngram-size" : 0
            , "min-length" : 1
            , "truncation-length" : 48
            }
        , "long markov" :
            { "base" : "markov"
            , "ngram-size" : 2
            , "min-length" : 8
            }
        , "portmanteau" :
            { "algorithm" : "portmanteau"
            , "size" : 2
            }
        , "pick a word" :
            { "algorithm" : "portmanteau"
            , "size" : 1
            }
        }
    }
, "filters" :
    { "default" : "none"
    , "presets" : 
        { "none" : {}
        , "original" : 
            { "min-length" : 3
            , "max-length" : 24
            , "original" : true
            }
        }
    }
}

{ "verbose" : false
, "number" : 1
, "infinity-threshold" : 10000
, "seed" : "auto"
, "gui" :
    { "enabled" : true
    }
, "lexicon" :
    { "uniform" : false
    , "ignore-case" : false
    , "files" :
      [ "namelists/viking_female.txt"
      , "namelists/viking_male.txt"
      , "namelists/viking_unknownGender.txt"
      ]
    }
, "tokenizer" :
    { "default" : "unicode"
    , "presets" :
        { "unicode" :
            { "algorithm" : "unicode"
            }
        , "english" :
            { "algorithm" : "ll1"
            , "token-list" : "eau,eue,ewe,ieu,iew,oeu,aa,ah,ae,ai,ao,au,aw,ay,ee,ei,ey,eo,eu,ew,ie,oa,oe,oi,oo,ou,ow,oy,ue,ui,uu,uy,dge,dgi,dgy,pph,rrh,sce,sci,scy,sch,tch,xce,xci,bb,cc,ch,ck,dd,dh,ff,gg,gh,kk,kh,ll,mm,nn,ng,pp,ph,qu,rr,rh,sh,ss,sw,tt,th,vv,wh,wr,xh,zz,a,b,c,d,e,f,g,h,i,j,k,l,m,n,o,p,q,r,s,t,u,v,w,x,y,z"

            }
        }
    }
, "generator" :
    { "default" : "markov"
    , "presets" :
        { "markov" :
            { "algorithm" : "markov"
            , "ngram-size" : 3
            , "min-length" : 3
            , "truncation-length" : 48
            }
        , "smooth-markov" :
            { "algorithm" : "smooth-markov"
            , "ngram-size" : 2
            , "min-length" : 3
            , "truncation-length" : 48
            }
        , "noise" :
            { "base" : "markov"
            , "ngram-size" : 0
            }
        , "long-markov" :
            { "base" : "markov"
            , "ngram-size" : 2
            , "min-length" : 8
            }
        }
    }
, "filters" :
    { "default" : "original"
    , "presets" : 
        { "original" : 
            { "min-length" : 3
            , "max-length" : 24
            , "original" : true
            }
        }
    }
}
